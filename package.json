{
	"name": "vanilla-web-components",
	"version": "1.0.0",
	"description": "Web components library with vanilla JavaScript.",
	"repository": {
		"type": "git",
		"url": "git@gitlab.com:naylalabs/web/vanilla-web-components.git"
	},
	"main": "./dist/index.html",
	"typings": "./dist",
	"scripts": {
		"build:prod": "webpack --mode production",
		"build:dev": "webpack --mode development",
		"start:dev": "webpack-dev-server --mode development",
		"postinstall": "husky install .husky",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.js\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.js\"",
		"lint:fix": "eslint \"{src,apps,libs,test}/**/*.js\" --fix",
	},
	"keywords": [
		"naylalabs",
		"javascript",
		"frontend",
		"web components"
	],
	"author": "Sabri Ayes <sabri@naylalabs.com>",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.17.7",
		"@babel/eslint-parser": "^7.5.4",
		"css-loader": "^6.5.1",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"filemanager-webpack-plugin": "^6.1.7",
		"glob": "^7.2.0",
		"happy-dom": "^2.55.0",
		"html-loader": "^3.0.1",
		"husky": "^7.0.4",
		"path": "^0.12.7",
		"postcss": "^8.4.5",
		"postcss-loader": "^6.2.1",
		"prettier": "^2.3.2",
		"raw-loader": "^4.0.2",
		"sass": "^1.45.1",
		"sass-loader": "^12.4.0",
		"sass-to-string": "^1.6.0",
		"webpack": "^5.64.1",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.5.0"
	}
}
